{
    "name": "jaspaul/laravel-rollout",
    "description": "A package to integrate rollout into your Laravel project.",
    "keywords": ["laravel", "rollout"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul.b@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "illuminate/cache": "^5.3|^5.4",
        "illuminate/support": "^5.3|^5.4",
        "opensoft/rollout": "^2.2"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.9",
        "phpunit/phpunit": "^6.0",
        "satooshi/php-coveralls": "^1.0",
        "squizlabs/php_codesniffer": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Jaspaul\\LaravelRollout\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
